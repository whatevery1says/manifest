{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "definitions": {
    "name": {
      "type": "string",
      "pattern": "RawData",
      "additionalProperties": false
    },
    "altTitle": {
      "type": "string",
      "additionalProperties": false
    },
    "date": {
      "type": "array",
      "items": {
        "oneOf": [
          {
            "type": "string",
            "format": "date"
          },
          {
            "type": "string",
            "format": "date-time"
          },
          {
            "$ref": "#/definitions/daterange"
          },
          {
            "$ref": "#/definitions/precisedaterange"
          },
          {
            "$ref": "#/definitions/normal"
          },
          {
            "$ref": "#/definitions/precise"
          }
        ]
      },
      "required": [
        "date"
      ],
      "additionalProperties": false
    },
    "daterange": {
      "type": "object",
      "properties": {
        "start": {
          "type": "string",
          "format": "date"
        },
        "end": {
          "type": "string",
          "format": "date"
        }
      },
      "required": [
        "start",
        "end"
      ],
      "additionalProperties": false
    },
    "description": {
      "type": "string",
      "additionalProperties": false
    },
    "group": {
      "type": "string",
      "additionalProperties": false
    },
    "label": {
      "type": "string",
      "additionalProperties": false
    },
    "namespace": {
      "type": "string",
      "default": "we1sv1.1",
      "additionalProperties": false
    },
    "normal": {
      "type": "object",
      "properties": {
        "normal": {
          "type": "array",
          "items": {
            "oneOf": [
              {
                "type": "string",
                "format": "date"
              },
              {
                "$ref": "#/definitions/daterange"
              }
            ]
          }
        }
      },
      "additionalProperties": false
    },
    "note": {
      "type": "string",
      "additionalProperties": false
    },
    "notes": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/note"
      },
      "required": [
        "notes"
      ],
      "additionalProperties": false
    },
    "path": {
      "type": "string",
      "pattern": "^,Corpus,[a-zA-Z-_,]+,$",
      "additionalProperties": false
    },
    "precise": {
      "type": "object",
      "properties": {
        "precise": {
          "type": "array",
          "items": {
            "oneOf": [
              {
                "type": "string",
                "format": "date-time"
              },
              {
                "$ref": "#/definitions/precisedaterange"
              }
            ]
          }
        }
      },
      "additionalProperties": false
    },
    "precisedaterange": {
      "type": "object",
      "properties": {
        "start": {
          "type": "string",
          "format": "date-time"
        },
        "end": {
          "type": "string",
          "format": "date-time"
        }
      },
      "required": [
        "start",
        "end"
      ],
      "additionalProperties": false
    },
    "refLocation": {
      "type": "string",
      "additionalProperties": false
    },
    "title": {
      "type": "string",
      "additionalProperties": false
    }
  },
  "type": "object",
  "properties": {
    "OCR": {
      "type": "boolean"
    },
    "relationships": {
      "type": "array"
    },
    "rights": {
      "type": "string"
    }
  },
  "required": [
    "name",
    "namespace",
    "path"
  ],
  "additionalProperties": true
}