{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "definitions": {
    "date-only": {
      "type": "string",
      "format": "date"
    },
    "date-time-only": {
      "type": "string",
      "format": "date"
    },
    "date-range": {
      "type": "object",
      "properties": {
        "start": {
          "type": "string",
          "format": "date"
        },
        "end": {
          "type": "string",
          "format": "date"
        }
      },
      "required": [
        "start"
      ]
    },
    "date-time-range": {
      "type": "object",
      "properties": {
        "start": {
          "type": "string",
          "format": "date-time"
        },
        "end": {
          "type": "string",
          "format": "date-time"
        }
      },
      "required": [
        "start"
      ]
    },
    "normal-date": {
      "type": "object",
      "properties": {
        "normal": {
          "anyOf": [
            {
              "$ref": "#/definitions/date-only"
            },
            {
              "$ref": "#/definitions/date-range"
            }
          ]
        }
      }
    },
    "precise-date": {
      "type": "object",
      "properties": {
        "normal": {
          "anyOf": [
            {
              "$ref": "#/definitions/date-time-only"
            },
            {
              "$ref": "#/definitions/date-time-range"
            }
          ]
        }
      }
    },
    "date": {
      "anyOf": [
        {
          "$ref": "#/definitions/date-only"
        },
        {
          "$ref": "#/definitions/date-time-only"
        },
        {
          "$ref": "#/definitions/date-range"
        },
        {
          "$ref": "#/definitions/date-time-range"
        },
        {
          "$ref": "#/definitions/normal-date"
        },
        {
          "type": "array",
          "items": {
            "$ref": "#/definitions/date"
          }
        }
      ]
    }
  },
